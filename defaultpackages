  sudo pacman -S i3-gaps git base-devel ttf-font-awesome easyeffects blueberry rofi code discord
  git clone https://aur.archlinux.org/yay.git
  cd yay
  makepkg -si
  yay -S polybar google-chrome spotify picom-ibhagwan-git
